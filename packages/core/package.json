{
  "name": "@typeb/core",
  "description": "Core package for typebox-extra",
  "license": "MIT",
  "version": "0.1.0",
  "author": "Michael Lazuardy",
  "keywords": [
    "typebox",
    "typebox-extra",
    "typebox-i18n",
    "typebox custom messages"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "locales"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/mlazuardy/typebox-extra"
  },
  "publishConfig": {
